<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata minClientVersion="2.12">
    <id>xunit.runner.reporters</id>
    <version>99.99.99-dev</version>
    <title>xUnit.net [Runner Reporters]</title>
    <authors>James Newkirk,Brad Wilson</authors>
    <summary>Includes runner reporters: TeamCity, AppVeyor, Verbose and Quiet.</summary>
    <description>Includes runner reporters for TeamCity, AppVeyor, Verbose and Quiet output for test runners. Supports .NET 4.5.2 or later, .NET Standard 1.1, .NET Standard 1.5, and .NET Core 1.0 or later.</description>
    <language>en-US</language>
    <projectUrl>https://github.com/xunit/xunit</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/xunit/media/master/logo-512-transparent.png</iconUrl>
    <licenseUrl>https://raw.githubusercontent.com/xunit/xunit/master/license.txt</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>
      <group targetFramework="net472">
        <dependency id="xunit.runner.utility" version="[99.99.99-dev]" />
      </group>
      <group targetFramework="netstandard2.0">
        <dependency id="NETStandard.Library" version="2.0.3" />
        <dependency id="xunit.runner.utility" version="[99.99.99-dev]" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="xunit.runner.reporters\bin\$Configuration$\net472\xunit.runner.reporters.net472.dll" target="lib\net472\" />

    <file src="xunit.runner.reporters\bin\$Configuration$\netstandard2.0\xunit.runner.reporters.netstandard20.dll" target="lib\netstandard2.0\" />
  </files>
</package>