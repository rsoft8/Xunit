<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata minClientVersion="2.8.5">
    <id>xunit.runner.reporters</id>
    <version>99.99.99-dev</version>
    <title>xUnit.net [Runner Reporters]</title>
    <authors>James Newkirk,Brad Wilson</authors>
    <description>
      Includes runner reporters for TeamCity, AppVeyor, Verbose and Quiet output for test runners.

      Supported platforms:
      - Desktop .NET 4.5+
      - .NET Core (Universal Windows Apps 10+, DNX Core 5+)
    </description>
    <language>en-US</language>
    <projectUrl>https://github.com/xunit/xunit</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/xunit/media/master/logo-512-transparent.png</iconUrl>
    <licenseUrl>https://raw.githubusercontent.com/xunit/xunit/master/license.txt</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>
      <group targetFramework="net45">
        <dependency id="xunit.runner.utility" version="99.99.99-dev" />
      </group>
      <group targetFramework="dotnet">
        <dependency id="Newtonsoft.Json" version="6.0.6" />
        <dependency id="xunit.runner.utility" version="99.99.99-dev" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="xunit.runner.reporters.desktop\bin\Release\xunit.runner.reporters.desktop.dll" target="lib\net45\" />
    <file src="xunit.runner.reporters.DotNetCore\bin\Release\xunit.runner.reporters.DotNetCore.dll" target="lib\dotnet\" />
  </files>
</package>
